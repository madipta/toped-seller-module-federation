{
  "name": "chat",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/chat/src",
  "prefix": "toped-seller",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/chat",
        "index": "apps/chat/src/index.html",
        "main": "apps/chat/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/chat/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/chat/src/favicon.ico", "apps/chat/src/assets"],
        "styles": [
          "libs/core/base-ui/src/lib/styles/reset.css",
          "libs/core/base-ui/src/lib/styles/reset.matui.scss",
          "apps/chat/src/styles.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": ["libs/features/chat/src/lib/styles"]
        },
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/chat/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/chat/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-dev-server",
      "configurations": {
        "production": {
          "browserTarget": "chat:build:production"
        },
        "development": {
          "browserTarget": "chat:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4202,
        "publicHost": "http://localhost:4202"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "chat:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/chat/**/*.ts", "apps/chat/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/chat/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nrwl/angular:file-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "chat:build",
        "port": 4202
      },
      "configurations": {
        "development": {
          "buildTarget": "chat:build:development"
        },
        "production": {
          "buildTarget": "chat:build:production"
        }
      }
    }
  },
  "tags": []
}
